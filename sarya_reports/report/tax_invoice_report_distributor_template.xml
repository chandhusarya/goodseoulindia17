<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <template id="tax_invoice_body">
                <t t-set="l10n_in_einvoice_json" t-value="o._get_l10n_in_edi_response_json()"/>
                <style>
                .page {
                    font-family: Arial, Helvetica, sans-serif;
                    font-size:12px;
                    width:100%;
                }
                .header_table {
                    border-collapse:collapse;
                    font-size:13px;
                    border: 1px solid #484848;
                }
                .header_table td {
                    border: 1px solid #484848;
                }
                .product_table {
                    border-collapse:collapse;
                    font-size:12px;
                    border: 1px solid #ddd;
                }
                .product_table td {
                    border: 1px solid #484848;
                    text-align:center;
                    padding:5px;
                }
                .product_table thead {
                    font-weight: bold;
                }
                .other_table {
                    border-collapse:collapse;
                    font-size:12px;
                    border: 1px solid #484848;
                }
                .other_table td {
                    border: 1px solid #484848;
                    padding:5px;
                }
                .missing {
                    color:red;
                }

                </style>
    <!--                            <div>-->
                <t t-if="o.move_type == 'out_invoice' or o.move_type == 'out_refund'">
                    <table class="header_table" width="100%">
                        <tbody>
                            <tr>
                                <td style="padding:10px;">
                                    <p><b>Billing Location:</b></p>
                                    <b><span class="text-left" t-field="o.company_id.name"/></b><br/>
                                    <t t-if="o.company_id.street"><span t-field="o.company_id.street"/></t><br/>
                                    <t t-if="o.company_id.street2"><span t-field="o.company_id.street2"/></t><br/>
                                    <t t-if="o.company_id.city"><span t-field="o.company_id.city"/></t>, <t t-if="o.company_id.zip"><span t-field="o.company_id.zip"/></t><br/><br/>
                                    <t t-if="o.company_id.state_id"><span><b>STATE : </b><t t-esc="o.company_id.state_id.name"/></span></t><br/>
                                    <t t-if="o.company_id.state_id"><span><b>STATE CODE : </b><t t-esc="o.company_id.state_id.gst_code"/></span></t><br/>
                                    <t t-if="o.company_id.pan_no"><span><b>PAN # : </b><t t-esc="o.company_id.pan_no"/></span></t><br/>
                                    <t t-if="o.company_id.vat"><span><b>GST # : </b><t t-esc="o.company_id.vat"/></span></t><br/>
                                    <t t-if="o.company_id.cin_no"><span><b>CIN # : </b><t t-esc="o.company_id.cin_no"/></span></t><br/>
                                    <t t-if="o.company_id.fssai_no"><span><b>FSSAI # : </b><t t-esc="o.company_id.fssai_no"/></span></t>
                                </td>
                                <td style="padding:10px;">
                                    <t t-set="shipper_address_invoice" t-value="o.sale_order_id.picking_type_id.warehouse_id and o.sale_order_id.picking_type_id.warehouse_id.partner_id"/>
                                    <t t-set="shipper_address_company" t-value="env['res.partner'].search([('parent_id', '=', o.company_id.partner_id.id), ('type','=','other')], limit=1)"/>
                                    <t t-set="shipper_address" t-value="shipper_address_invoice or shipper_address_company"/>
                                    <p><b>Supplier's Location:</b></p>
                                    <b><span class="text-left" t-field="shipper_address.name"/></b><br/>
                                    <t t-if="shipper_address.street"><span t-field="shipper_address.street"/></t><br/>
                                    <t t-if="shipper_address.street2"><span t-field="shipper_address.street2"/></t><br/>
                                    <t t-if="shipper_address.city"><span t-field="shipper_address.city"/></t>, <t t-if="shipper_address.zip"><span t-field="shipper_address.zip"/></t><br/><br/>
                                    <t t-if="shipper_address.state_id"><span><b>STATE : </b><t t-esc="shipper_address.state_id.name"/></span></t><br/>
                                    <t t-if="shipper_address.state_id"><span><b>STATE CODE : </b><t t-esc="shipper_address.state_id.gst_code"/></span></t><br/>
                                    <t t-if="o.company_id.pan_no"><span><b>PAN # : </b><t t-esc="o.company_id.pan_no"/></span></t><br/>
                                    <t t-if="o.company_id.vat"><span><b>GST # : </b><t t-esc="o.company_id.vat"/></span></t><br/>
                                    <t t-if="o.company_id.cin_no"><span><b>CIN # : </b><t t-esc="o.company_id.cin_no"/></span></t><br/>
                                    <t t-if="o.company_id.fssai_no"><span><b>FSSAI # : </b><t t-esc="o.company_id.fssai_no"/></span></t>
                                </td>
                                <!--<td>
                                Comment until E Invoice is activated
                                    <t t-if="l10n_in_einvoice_json">
                                        <img t-att-src="'/report/barcode/?barcode_type=%s&amp;value=%s&amp;width=%s&amp;height=%s' %('QR', l10n_in_einvoice_json['SignedQRCode'], 500, 500)" style="max-height: 155px"/>
                                    </t>

                                </td>-->
                            </tr>
                        </tbody>
                    </table>
                </t>
                    <table class="header_table" width="100%">
                        <tbody>
                            <tr>
                                <td style="padding:10px;" width="35%">
                                    <p><b>Bill to - Buyer :</b></p>
                                    <b><span class="text-left" t-field="o.partner_id.name"/></b><br/>
                                    <t t-if="o.partner_id.street"><span t-field="o.partner_id.street"/></t>, <t t-if="o.partner_id.street2"><span t-field="o.partner_id.street2"/><br/>
                                    </t><t t-if="o.partner_id.city"><span t-field="o.partner_id.city"/></t>, <t t-if="o.partner_id.zip"><span t-field="o.partner_id.zip"/></t><br/><br/>
                                    <t t-if="o.partner_id.state_id"><span><b>STATE : </b><t t-esc="o.partner_id.state_id.name"/></span></t><br/>
                                    <t t-if="o.partner_id.state_id"><span><b>STATE CODE : </b><t t-esc="o.partner_id.state_id.gst_code"/></span></t><br/>
                                    <t t-if="o.partner_id.l10n_in_pan"><span><b>PAN # : </b><t t-esc="o.partner_id.l10n_in_pan"/></span></t><br/>
                                    <t t-if="o.partner_id.vat"><span><b>GST # : </b><t t-esc="o.partner_id.vat"/></span></t><br/>
                                    <t t-if="o.partner_id.fssai_no"><span><b>FSSAI # : </b><t t-esc="o.partner_id.fssai_no"/></span></t>
                                </td>
                                <td style="padding:10px;" width="35%">
                                    <p><b>Ship To :</b></p>
                                    <b><span class="text-left" t-field="o.partner_shipping_id.name"/></b><br/>
                                    <t t-if="o.partner_shipping_id.street"><span t-field="o.partner_shipping_id.street"/></t>, <t t-if="o.partner_shipping_id.street2"><span t-field="o.partner_shipping_id.street2"/><br/>
                                    </t><t t-if="o.partner_shipping_id.city"><span t-field="o.partner_shipping_id.city"/></t>, <t t-if="o.partner_shipping_id.zip"><span t-field="o.partner_shipping_id.zip"/></t><br/><br/>
                                    <t t-if="o.partner_shipping_id.state_id"><span><b>STATE : </b><t t-esc="o.partner_shipping_id.state_id.name"/></span></t><br/>
                                    <t t-if="o.partner_shipping_id.state_id"><span><b>STATE CODE : </b><t t-esc="o.partner_shipping_id.state_id.gst_code"/></span></t><br/>
                                    <t t-if="o.partner_shipping_id.l10n_in_pan"><span><b>PAN # : </b><t t-esc="o.partner_shipping_id.l10n_in_pan"/></span></t><br/>
                                    <t t-if="o.partner_shipping_id.vat"><span><b>GST # : </b><t t-esc="o.partner_shipping_id.vat"/></span></t><br/>
                                    <t t-if="o.partner_shipping_id.fssai_no"><span><b>FSSAI # : </b><t t-esc="o.partner_shipping_id.fssai_no"/></span></t>
                                    <t t-if="o.partner_shipping_id.delivery_contact"><span><b>CONTACT # : </b><t t-esc="o.partner_shipping_id.delivery_contact"/></span></t>
                                </td>
                                <td style="padding:5px;" width="30%">


                                    <t t-set="l10n_in_ewaybill_json" t-value="o._get_l10n_in_edi_ewaybill_response_json()"/>
                                    <p><b>Invoice No :</b><t t-esc="o.name"/> </p>
                                    <p><b>Invoice Date :</b><t t-if="o.invoice_date" t-esc="o.invoice_date.strftime('%d/%m/%Y')"/></p>
                                    <p><b>PO Number :</b><t t-esc="o.ref"/> </p>
    <!--                                                <p><b>PO Date :</b><span class="missing">####</span></p>-->
                                    <p><b>Delivery No :</b><t t-esc="o.do_number"/></p>
                                    <p><b>Delivery Date :</b><t t-if="o.actual_delivery_date" t-esc="o.actual_delivery_date.strftime('%d/%m/%Y')"/></p>
                                    <p t-if="l10n_in_einvoice_json"><b>IRN No :</b>
                                        <span t-esc="l10n_in_einvoice_json['Irn']"/>
                                    </p>
                                    <p t-if="l10n_in_ewaybill_json"><b>Ewaybill No:</b>
                                        <p t-if="'EwbNo' in l10n_in_ewaybill_json" class="m-0" t-esc="l10n_in_ewaybill_json['EwbNo']"/>
                                        <p t-if="'ewayBillNo' in l10n_in_ewaybill_json" class="m-0" t-esc="l10n_in_ewaybill_json['ewayBillNo']"/>
                                        Until: <t t-if="'EwbValidTill' in l10n_in_ewaybill_json" class="m-0" t-esc="l10n_in_ewaybill_json['EwbValidTill']"/>
                                        <t t-if="'validUpto' in l10n_in_ewaybill_json" class="m-0" t-esc="l10n_in_ewaybill_json['validUpto']"/>
                                    </p>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                <table class="product_table" width="100%">
                    <thead>
                        <tr>
                            <t t-if="o.move_type == 'out_invoice' or o.move_type == 'out_refund'">
                                <td colspan="9">PRODUCT DETAILS</td>
                            </t>
                            <t t-if="o.move_type == 'in_invoice' or o.move_type == 'in_refund'">
                                <td colspan="7">PRODUCT DETAILS</td>
                            </t>
    <!--                                        <td colspan="3">SCHEME</td>-->
                            <td colspan="2">GST</td>
                        </tr>
                        <tr>
                            <td>SL #</td>
                            <td>DESCRIPTION</td>
                            <t t-if="o.move_type == 'out_invoice' or o.move_type == 'out_refund'">
                                <td>HSN CODE</td>
                                <td>BATCH NO   <br/>Exp. Date</td>
                            </t>
                            <td>QTY</td>
                            <td>UQC</td>
                            <td>Case Rate</td>
    <!--                                        <td>%</td>-->
    <!--                                        <td>Amount</td>-->
                            <td>Net Amt.</td>
                            <td>%</td>
                            <td>Amount</td>
                        </tr>
                    </thead>
                    <tbody>
                        <t t-set="total_qty" t-value="[]"/>
                        <t t-set="product_wise_lots" t-value="o.product_wise_lot()"/>
                        <t t-set="total_qty" t-value="0"/>
                        <t t-set="total_tax_amt" t-value="0"/>
                        <t t-set="current_subtotal" t-value="0"/>
                        <tr t-foreach="o.invoice_line_ids" t-as="line">
                            <td><span t-esc="line_index+1"/> </td>
                            <td><span t-esc="line.name"/> </td>
<!--                            <td><span t-esc="'{0:,.2f}'.format(line.pkg_unit_price)"/> </td>-->
                            <t t-if="o.move_type == 'out_invoice' or o.move_type == 'out_refund'">
                                <td><span t-esc="line.product_id.l10n_in_hsn_code"/> </td>
                                <t t-set="lot_name" t-value="', '.join([(str(lot['lot_name'])) for lot in product_wise_lots if lot['product_id'] == line.product_id.id])"/>
                                <t t-set="lot_expiry" t-value="', '.join([(str(lot['expiration_date'])) for lot in product_wise_lots if lot['product_id'] == line.product_id.id])"/>
                                <td>
                                    <span t-if="lot_name" t-esc="lot_name"/><br/>
                                    <span t-if="lot_expiry" t-esc="lot_expiry"/>
                                </td>
                            </t>
                            <t t-set="total_qty" t-value="line.product_packaging_qty+total_qty"/>
                            <td><span t-esc="line.product_packaging_qty"/> </td>
                            <td><span t-esc="line.package_id.name"/> </td>
                            <td><span t-esc="'{0:,.2f}'.format(line.pkg_unit_price)"/> </td>
    <!--                                        <td><span class="missing">####</span> </td>-->
    <!--                                        <td><span class="missing">####</span> </td>-->
                            <td><t t-esc="'{0:,.2f}'.format(line.price_subtotal)"/> </td>
                            <t t-set="current_subtotal" t-value="current_subtotal + line.price_subtotal"/>
                            <t t-set="taxes" t-value="', '.join([(tax.invoice_label or tax.name) for tax in line.tax_ids])"/>
                            <td><span t-out="taxes" id="line_tax_ids">Tax 15%</span> </td>
                            <t t-set="total_tax_amt" t-value="line.tax_amount+total_tax_amt"/>
                            <td><t t-esc="'{0:,.2f}'.format(line.tax_amount)"/> </td>
                        </tr>
                        <tr>
                            <td><b>Total</b> </td>
                            <t t-if="o.move_type == 'out_invoice' or o.move_type == 'out_refund'">
                                <td></td>
                                <td></td>
                            </t>
                            <td></td>
                            <td><t t-esc="total_qty"/></td>
                            <td></td>
                            <td></td>
                            <td><span t-out="'{0:,.2f}'.format(current_subtotal)">0.0</span></td>
                            <td></td>
    <!--                                        <td></td>-->
    <!--                                        <td></td>-->
                            <td></td>
                        </tr>
                    </tbody>
                </table>
                <table class="other_table" width="100%">
                    <tbody>
                        <t t-set="tax_total" t-value="0"/>
                        <tr>
                            <td width="45%" rowspan="5">
                                <span>This is a Computer generated document</span><br/>
                                <span>Net Wt ( Kg ) :<t t-esc="o.total_product_weight"/> </span><br/>
                                <span>Gross Wt ( Kg) :<t t-esc="o.total_product_gross_weight"/></span><br/><br/>
                                <span><b>Transporter Information</b></span><br/>
                                <t t-if="o.l10n_in_transporter_id"><span>Transporter Name : <t t-esc="o.l10n_in_transporter_id.name"/></span><br/></t>
                                <t t-if="o.l10n_in_vehicle_no"><span>Truck Number : <t t-esc="o.l10n_in_vehicle_no"/></span><br/></t>
                                <t t-if="l10n_in_vehicle_type"><span>Transport type :
                                    <t t-if="l10n_in_vehicle_type == 'R'">Regular</t>
                                    <t t-if="l10n_in_vehicle_type == 'O'">ODC</t>
                                </span><br/><br/></t>
                            </td>
                            <td width="40%"><span>SUB TOTAL :</span></td>

                            <td>
                                <span t-out="(current_subtotal)" t-options='{"widget": "monetary", "display_currency": o.currency_id}'>$500.00</span>
                            </td>
                        </tr>
                        <tr>
                            <td width="40%"><span>CGST :</span></td>
                            <t t-if="o.get_tax_group('CGST')" t-set="tax_total" t-value="tax_total + o.get_tax_group('CGST')"/>
                            <td><span t-if="o.get_tax_group('CGST')" t-esc="'{0:,.2f}'.format(o.get_tax_group('CGST'))"/></td>
                        </tr>
                        <tr>
                            <td><span>SGST/UGST :</span></td>
                            <t t-if="o.get_tax_group('SGST')" t-set="tax_total" t-value="tax_total + o.get_tax_group('SGST')"/>
                            <td><span t-if="o.get_tax_group('SGST')" t-esc="'{0:,.2f}'.format(o.get_tax_group('SGST'))"/></td>
                        </tr>
                        <tr>
                            <td><span>IGST :</span></td>
                            <t t-if="o.get_tax_group('IGST')" t-set="tax_total" t-value="o.get_tax_group('IGST')"/>
                            <td><span t-if="o.get_tax_group('IGST')" t-esc="'{0:,.2f}'.format(o.get_tax_group('IGST'))"/></td>
                        </tr>
                        <tr>
                            <td><span>TOTAL TAX :</span></td>
                            <td><span t-out="(tax_total)"  t-options='{"widget": "monetary", "display_currency": o.currency_id}'>0.0</span></td>
                        </tr>
                        <!--<tr>
                            <td><span>DISCOUNT :</span></td>
                            <td></td>
                        </tr>-->
                        <tr>
                            <td/>
                            <td><b>GRAND TOTAL:</b></td>
                            <td><span t-out="o.amount_total_in_currency_signed"  t-options='{"widget": "monetary", "display_currency": o.currency_id}'>0.0</span></td>
                        </tr>
                        <tr>
                            <td colspan="3"><b>Amount In Word :
                                <small class="text-muted lh-sm"><span t-esc="o.sar_num2words(o.amount_total_in_currency_signed)"/></small>
                                </b>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="3">
                                <t t-set="bank_account" t-value="request.env['res.partner.bank'].search([('partner_id', '=', o.company_id.partner_id.id)], limit=1)"/>
                                <b>Bank Details :</b>
                                <t t-if="bank_account">
                                    <t t-if="bank_account.bank_id" t-esc="bank_account.bank_id.name"/>,
                                     A/C NO.: <t t-esc="bank_account.acc_number"/>,
                                     IFSC CODE: <t t-esc="bank_account.bank_id.bic"/>, <t t-esc="bank_account.bank_id.street"/>
                                </t>

                            </td>
                        </tr>
                        <!--<tr>
                            <td colspan="2">
                                <span>Date and Time of Preparation of Invoice:</span><br/>
                                <span>Date and Time of Removal of Goods:</span>
                            </td>
                            <td/>
                        </tr>-->
                    </tbody>
                </table>

                <table class="other_table" width="100%">
                    <tbody>
                        <tr>
                            <td width="75%">
                                <t t-if="o.move_type == 'out_invoice'"><p>
                                    I/WE CERTIFY THAT THE FOODS MENTIONED IN THIS INVOICE IS/ARE WARRANTED TO BE OF THE NATURE &amp; QUALITY WHICH IT / THESE PURPORTS /PUPORTS TO BE.
                                </p></t>
                            </td>
                            <td  width="25%">
                                <span>For</span><br/>
                                <t t-esc="o.company_id.name"/>
                            </td>
                        </tr>
                    </tbody>
                </table>
        </template>

        <template id="tax_invoice_original">
            <template id="report_header_custom" inherit_id="web.external_layout_header">
                <xpath expr="//div[@class='header']" position="replace">
                    <div class="header col-xs-12">
                        <div class="col-xs-4" style="min-width:33%;padding: 0px; margin: 0px; display: inline-block;">
                            <img style="padding: 0px; margin: 0px; max-width: 100%;"
                                 t-att-src="image_data_uri(o.company_id.logo)" width="100px" height="100px"/>
                        </div>
                        <div class="col-xs-5" style="min-width:33%;text-align: center;min-height:30px;display: flex;justify-content: center;display: inline-block;align-items: center;">
                            <span t-if="o.move_type == 'out_invoice' and o.state == 'posted'"><h4>TAX INVOICE</h4></span>
                            <span t-elif="o.move_type == 'out_invoice' and o.state == 'draft'"><h4>Draft Invoice</h4></span>
                            <span t-elif="o.move_type == 'out_invoice' and o.state == 'cancel'"><h4>Cancelled Invoice</h4></span>
                            <span t-elif="o.move_type == 'out_refund' and o.state == 'posted'"><h4>Credit Note</h4></span>
                            <span t-elif="o.move_type == 'out_refund' and o.state == 'draft'"><h4>Draft Credit Note</h4></span>
                            <span t-elif="o.move_type == 'out_refund' and o.state == 'cancel'"><h4>Cancelled Credit Note</h4></span>
                            <span t-elif="o.move_type == 'in_refund'"><h4>Vendor Credit Note</h4></span>
                            <span t-elif="o.move_type == 'in_invoice'"><h4>Vendor Bill</h4></span>
                        </div>
                        <div class="col-xs-3 pull-left" style="float:right;min-width:33%;display: inline-block;font-size: 10pt;line-height: 13px;">
                            <br/>
                        </div>
                    </div>
                </xpath>
            </template>
            <t t-call="web.external_layout">
            <div class="oe_structure"/>
                <div class="page">
                    <t t-call="sarya_reports.tax_invoice_body"/>
                </div>
            </t>
        </template>

        <template id="tax_invoice_duplicate">
            <div class="header col-xs-12">
                <div class="col-xs-4" style="min-width:33%;padding: 0px; margin: 0px; display: inline-block;">
                    <img style="padding: 0px; margin: 0px; max-width: 100%;"
                         t-att-src="image_data_uri(o.company_id.logo)" width="100px" height="100px"/>
                </div>
                <div class="col-xs-5" style="min-width:33%;text-align: center;min-height:30px;display: flex;justify-content: center;display: inline-block;align-items: center;">
                    <span t-if="o.move_type == 'out_invoice' and o.state == 'posted'"><h4>TAX INVOICE</h4></span>
                    <span t-elif="o.move_type == 'out_invoice' and o.state == 'draft'"><h4>Draft Invoice</h4></span>
                    <span t-elif="o.move_type == 'out_invoice' and o.state == 'cancel'"><h4>Cancelled Invoice</h4></span>
                    <span t-elif="o.move_type == 'out_refund' and o.state == 'posted'"><h4>Credit Note</h4></span>
                    <span t-elif="o.move_type == 'out_refund' and o.state == 'draft'"><h4>Draft Credit Note</h4></span>
                    <span t-elif="o.move_type == 'out_refund' and o.state == 'cancel'"><h4>Cancelled Credit Note</h4></span>
                    <span t-elif="o.move_type == 'in_refund'"><h4>Vendor Credit Note</h4></span>
                    <span t-elif="o.move_type == 'in_invoice'"><h4>Vendor Bill</h4></span>
                </div>
                <div class="col-xs-3 pull-left" style="float:right;min-width:33%;display: inline-block;font-size: 10pt;line-height: 13px;">
                    <br/>
<!--                    <span>Duplicate for Transporter</span><br/>-->
                </div>
            </div>
            <div class="page">
                <t t-call="sarya_reports.tax_invoice_body"/>
            </div>
        </template>

        <template id="tax_invoice_triplicate">
            <div class="header col-xs-12">
                <div class="col-xs-4" style="min-width:33%;padding: 0px; margin: 0px; display: inline-block;">
                    <img style="padding: 0px; margin: 0px; max-width: 100%;"
                         t-att-src="image_data_uri(o.company_id.logo)" width="100px" height="100px"/>
                </div>
                <div class="col-xs-5" style="min-width:33%;text-align: center;min-height:30px;display: flex;justify-content: center;display: inline-block;align-items: center;">

                    <span t-if="o.move_type == 'out_invoice' and o.state == 'posted'"><h4>TAX INVOICE</h4></span>
                    <span t-elif="o.move_type == 'out_invoice' and o.state == 'draft'"><h4>Draft Invoice</h4></span>
                    <span t-elif="o.move_type == 'out_invoice' and o.state == 'cancel'"><h4>Cancelled Invoice</h4></span>
                    <span t-elif="o.move_type == 'out_refund' and o.state == 'posted'"><h4>Credit Note</h4></span>
                    <span t-elif="o.move_type == 'out_refund' and o.state == 'draft'"><h4>Draft Credit Note</h4></span>
                    <span t-elif="o.move_type == 'out_refund' and o.state == 'cancel'"><h4>Cancelled Credit Note</h4></span>
                    <span t-elif="o.move_type == 'in_refund'"><h4>Vendor Credit Note</h4></span>
                    <span t-elif="o.move_type == 'in_invoice'"><h4>Vendor Bill</h4></span>
                </div>
                <div class="col-xs-3 pull-left" style="float:right;min-width:33%;display: inline-block;font-size: 10pt;line-height: 13px;">
                    <br/>
<!--                    <span>Triplicate for Supplier</span><br/>-->
                </div>
            </div>
            <div class="page">
                <t t-call="sarya_reports.tax_invoice_body"/>
            </div>
        </template>


        <template id="report_tax_invoice_distributor" >
            <t t-call="web.html_container">
<!--                <t t-call="web.external_layout">-->
                <t t-call-assets="web.assets_common"/>
                <t t-foreach="docs" t-as="o">
                    <t t-call="sarya_reports.tax_invoice_original"/>
                    <!--<div style="page-break-after: always;"/>
                    <t t-call="sarya_reports.tax_invoice_duplicate"/>
                    <div style="page-break-after: always;"/>
                    <t t-call="sarya_reports.tax_invoice_triplicate"/>-->
                </t>
<!--                </t>-->
            </t>
        </template>

    </data>
</odoo>