<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_partner_stock_form_inherit" model="ir.ui.view">
            <field name="name">res.partner.form.inh</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="stock.view_partner_stock_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='property_stock_supplier']" position="after">
                    <field name="picking_type_id" domain="[('code','=','outgoing')]"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>