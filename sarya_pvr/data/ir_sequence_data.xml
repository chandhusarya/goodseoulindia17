<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_pvr_stock_request" model="ir.sequence">
            <field name="name">PVR Stock Request Sequence</field>
            <field name="code">pvr.stock.request</field>
            <field name="prefix">PVR/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="None"/>
        </record>
    </data>

    <data noupdate="1">
        <record id="seq_container_request" model="ir.sequence">
            <field name="name">Container Request Sequence</field>
            <field name="code">container.request</field>
            <field name="prefix">CR/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="None"/>
        </record>
    </data>

    <data noupdate="1">
        <record id="seq_closing_container_request" model="ir.sequence">
            <field name="name">Closing Session Requests</field>
            <field name="code">closing.session</field>
            <field name="prefix">CLR/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="None"/>
        </record>
    </data>

    <data noupdate="1">
        <record id="seq_container_transfer" model="ir.sequence">
            <field name="name">Container Transfer</field>
            <field name="code">container.transfer</field>
            <field name="prefix">CT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="None"/>
        </record>
    </data>

    <data noupdate="1">
        <record id="seq_wastage_entry_pvr" model="ir.sequence">
            <field name="name">Wastage Entry</field>
            <field name="code">pvr.wastage.entry</field>
            <field name="prefix">WE/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="None"/>
        </record>
    </data>




</odoo>
