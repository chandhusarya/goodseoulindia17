        <field name="model">customer.sub.classification2</field>
        <field name="arch" type="xml">
            <form string="Trade Channel Form">
                <sheet>
                    <group name="region_form main">
                        <group name="region_form_child">
                            <field name="name" required="1"/>
                            <field name="classification_id"  domain="[('trade_channel_id','=',trade_channel_id)]"  required="1"/>
                        </group>
                        <group name="region_form_child">
                             <field name="trade_channel_id" required="1"/>
                             <field name="sub_classification_id"  domain="[('classification_id','=',classification_id)]"  required="1"/>
                        </group>
                    </group>
                </sheet>
                
            </form>
        </field>
    </record>

    <record id="sub_classific2_menu_action" model="ir.actions.act_window">
