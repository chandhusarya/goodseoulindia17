    <menuitem name="Sub Classification2" id="sub_classifi2_chnll_menu" action="sub_classific2_menu_action" sequence="6" parent="kg_sarya.customer_classifi_menu"/>



    <record id="sub_classification3_tree" model="ir.ui.view">
        <field name="name">customer.sub.classification3.tree</field>
        <field name="model">customer.sub.classification3</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" required="1"/>
                <field name="trade_channel_id" required="1"/>
                <field name="classification_id" required="1"/>
                <field name="sub_classification_id"/>
                <field name="sub_classification2_id"/>
                <field name="picking_type_id"/>
            </tree>
        </field>
    </record>

    <record id="sub_classification3_form" model="ir.ui.view">
        <field name="name">customer.sub.classification3.form</field>
        <field name="model">customer.sub.classification3</field>
        <field name="arch" type="xml">
            <form string="Trade Channel Form">
                <sheet>
                    <group name="region_form main">
                        <group name="region_form_child">
                            <field name="name" required="1"/>
                            <field name="classification_id"
                                   domain="[('trade_channel_id','=',trade_channel_id)]"
                                   required="1"/>
                            <field name="sub_classification2_id" domain="[('sub_classification_id','=',sub_classification_id)]" />
                        </group>
                        <group name="region_form_child">
                            <field name="trade_channel_id" required="1"/>
                            <field name="sub_classification_id"
                                   domain="[('classification_id','=',classification_id)]"
                                   required="1"/>
                            <field name="picking_type_id"/>
                        </group>
                    </group>
                </sheet>

            </form>
        </field>
    </record>

    <record id="sub_classification3_menu_action" model="ir.actions.act_window">
        <field name="name">Sub Classification3</field>
        <field name="res_model">customer.sub.classification3</field>
        <field name="view_mode">tree,form</field>
