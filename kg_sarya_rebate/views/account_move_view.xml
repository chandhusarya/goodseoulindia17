<?xml version="1.0" encoding="utf-8"?><odoo>    <data>        <record model="ir.ui.view" id="account_line_margin_inherit">            <field name="name">account.line.margin.inherit</field>            <field name="model">account.move</field>            <field name="inherit_id" ref="account.view_move_form"/>            <field name="arch" type="xml">                <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='price_unit']" position="after">                    <field name="line_margin"/>                    <field name="margin_subtotal_signed" column_invisible="True"/>                    <field name="purchase_price" column_invisible="True"/>                </xpath>                <xpath expr="//page[@name='other_info']" position="after">                    <page id="rebate" string="Rebate" name="rebate_entries">                        <group id="fixed_rebate_line">                            <field name="fixed_rebate_line_ids" nolabel="1" readonly="1">                                <tree>                                    <field name="account_id"/>                                    <field name="partner_id"/>                                    <field name="name"/>                                    <field name="debit"/>                                    <field name="credit"/>                                    <field name="residual"/>                                </tree>                            </field>                        </group>                    </page>                </xpath>            </field>        </record>        <record model="ir.ui.view" id="margin_view_pivot">            <field name="name">margin.pivot.inherit</field>            <field name="model">account.invoice.report</field>            <field name="inherit_id" ref="account.view_account_invoice_report_pivot"/>            <field name="arch" type="xml">                <xpath expr="//field[@name='price_subtotal']" position="after">                    <field name="purchase_price" type="measure"/>                    <field name="margin_subtotal_signed" type="measure"/>                </xpath>            </field>        </record>        <record id="margin_view_graph" model="ir.ui.view">            <field name="name">margin.graph.inherit</field>            <field name="model">account.invoice.report</field>            <field name="inherit_id" ref="account.view_account_invoice_report_graph"/>            <field name="arch" type="xml">                <xpath expr="//field[@name='price_subtotal']" position="after">                    <field name="purchase_price" type="measure"/>                    <field name="margin_subtotal_signed" type="measure"/>                </xpath>            </field>        </record>    </data></odoo>