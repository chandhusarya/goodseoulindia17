<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">

		<!-- Sequences for sales.return.form -->
		<record id="seq_sales_return_form" model="ir.sequence">
			<field name="name">Sales Return Form</field>
			<field name="code">sales.return.form</field>
			<field name="prefix">SR/%(year)s</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False"/>
		</record>
		<record id="stock_transfer_internal_seq" model="ir.sequence">
            <field name="name">Stock Transfer Internal</field>
            <field name="code">stock.transfers.sequence</field>
            <field name="active">TRUE</field>
            <field name="prefix">ST/IN/</field>
            <field name="padding">5</field>
            <field name="number_increment">1</field>
        </record>

		<record id="load_request_internal_seq" model="ir.sequence">
            <field name="name">Load Request</field>
            <field name="code">stock.transfer.request</field>
            <field name="active">TRUE</field>
            <field name="prefix">LD</field>
            <field name="padding">3</field>
            <field name="number_increment">1</field>
        </record>

	</data>
</odoo>
