<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="product_supplierinfo_tree_view_inherit_discount" model="ir.ui.view">
        <field name="name">product.supplierinfo.tree.inherit</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.product_supplierinfo_tree_view"/>
        <field name="arch" type="xml">
            <field name="delay" position="before">
                <field name="discount_1" widget="percentage" optional="show"/>
                <field name="discount_2" widget="percentage" optional="show"/>
            </field>
        </field>
    </record>
</odoo>
