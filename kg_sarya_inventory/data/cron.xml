<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="update_inventory_value_history" model="ir.cron">
            <field name="name">Inventory Value History</field>
            <field eval="True" name="active" />
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="kg_sarya_inventory.model_inventory_value_history" />
            <field name="state">code</field>
            <field name="code">model.update_inventory_value()</field>
            <field name="priority" eval="5" />
        </record>


    </data>
</odoo>
