<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_shipment_advice_receive_qty_partial_form" model="ir.ui.view">
            <field name="name">shipment.advice.receive.qty.partial.form</field>
            <field name="model">shipment.advice.receive.qty.partial</field>
            <field name="arch" type="xml">
                <form string="Approval Required">
                    <group>
                        <field name="shipment_advice_id" invisible="1"/>
                        <field name="summary_lines" nolabel="1" colspan="2">
                            <tree editable="bottom">
                                <field name="product_id" readonly="1"/>
                                <field name="lot_id" readonly="1"/>
                                <field name="expiry_date" readonly="1"/>
                                <field name="production_date" readonly="1"/>
                                <field name="qty_received_in_inspection" readonly="1"/>
                                <field name="balance_qty_in_inspection" readonly="1"/>
                                <field name="qty_for_mainstock"/>
                            </tree>
                        </field>
                    </group>
                    <footer>
                        <button string="Transfer"
                                name="do_transfer"
                                type="object"
                                confirm="Item in 'Qty to Transfer' will be moved to main stock"
                                class="oe_highlight"/>
                        <button string="Cancel"
                                class="btn btn-secondary"
                                special="cancel"
                                data-hotkey="z"/>
                    </footer>
                </form>

            </field>
        </record>

    </data>
</odoo>