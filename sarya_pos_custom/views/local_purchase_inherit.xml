<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="local_purchase_form_view_inherit" model="ir.ui.view">
            <field name="name">local.purchase.form</field>
            <field name="model">local.purchase</field>
            <field name="inherit_id" ref="cha_sarya_purchase.local_purchase_form_view"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='pos_id']" position="attributes">
                    <attribute name="domain">[('terminal_type', '=', 'primary')]</attribute>
                </xpath>
            </field>
        </record>

    </data>
</odoo>