<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for orderpoint -->
        <record id="sequence_stock_inventory" model="ir.sequence">
            <field name="name">Stock Inventory</field>
            <field name="code">stock.inventory</field>
            <field name="prefix">SW/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id"></field>
        </record>

        <record id="sequence_pos_wastage" model="ir.sequence">
            <field name="name">POS Wastage</field>
            <field name="code">pos.wastage</field>
            <field name="prefix">WST/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id"></field>
        </record>

        <record id="sequence_outlet_transfer" model="ir.sequence">
            <field name="name">Outlet Transfer</field>
            <field name="code">outlet.transfer</field>
            <field name="prefix">OT/</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id"></field>
        </record>


    </data>
</odoo>